<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LoginPage.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:viewsUsers="clr-namespace:LoginPage.Views.Users"
    xmlns:viewsMusicians="clr-namespace:LoginPage.Views.Musicians"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        ContentTemplate="{DataTemplate viewsUsers:InitialPage}" Route="MainPage"/>
    <ShellContent
    ContentTemplate="{DataTemplate viewsUsers:LoginPage}" Route="LoginPage"/>

    <ShellContent
    ContentTemplate="{DataTemplate viewsMusicians:MusicianHomePage}" Route="MusicianHomePage"/>


    
</Shell>
